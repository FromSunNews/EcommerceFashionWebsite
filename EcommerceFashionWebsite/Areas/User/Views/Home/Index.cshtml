                    @_lang.Getkey("sale_bag")
                </p>
                <div class="product-price">
                    $35.00
                    <span>/ @_lang.Getkey("backpack")</span>
                </div>
            </div>
            <div class="countdown-timer" id="countdown">
                <div class="cd-item">
                    <span>56</span>
                    <p>@_lang.Getkey("days")</p>
                </div>
                <div class="cd-item">
                    <span>12</span>
                    <p>@_lang.Getkey("hours")</p>
                </div>
                <div class="cd-item">
                    <span>40</span>
                    <p>@_lang.Getkey("mins")</p>
                </div>
                <div class="cd-item">
                    <span>52</span>
                    <p>@_lang.Getkey("second")</p>
                </div>
            </div>
            <a href="#" class="primary-btn">@_lang.Getkey("shop_now")</a>
        </div>
    </div>
</section>
