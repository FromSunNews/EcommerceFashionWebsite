<!-- Deal Of The Week Section Begin-->
<section class="deal-of-week set-bg spad" data-setbg="@Url.Content("~/user/img/time-bg.jpg")">
    <div class="container">
        <div class="col-lg-6 text-center">
            <div class="section-title">
                <h2>@_lang.Getkey("deal_of")</h2>
